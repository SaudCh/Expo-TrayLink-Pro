{
  "expo": {
    "name": "Tray Link Pro",
    "slug": "traylinkpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#29B6F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ios.coach",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "The app accesses your camera to let you take photos to share with your friends.",
        "NSPhotoLibraryUsageDescription": "The app accesses your photos to let you share them with your friends."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#29B6F6"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d93feee8-75dc-4760-a3b1-5cbda3c5a2b8"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d93feee8-75dc-4760-a3b1-5cbda3c5a2b8"
      }
    }
  }
}
