{
  "expo": {
    "name": "Tray Link Pro",
    "slug": "traylinkpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#29B6F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ios.traylinkpro",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "The app accesses your camera to let you scan QR codes.",
        "NSPhotoLibraryUsageDescription": "The app accesses your photos to let you set them as your profile picture."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#29B6F6"
      },
      "package": "com.android.traylinkpro"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d93feee8-75dc-4760-a3b1-5cbda3c5a2b8"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow TrayLink Pro to access your photos to let you set them as your profile picture."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow TrayLink Pro to access your camera to scan QR codes."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d93feee8-75dc-4760-a3b1-5cbda3c5a2b8"
      }
    }
  }
}
